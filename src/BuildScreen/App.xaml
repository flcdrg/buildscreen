<Application x:Class="BuildScreen.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:BuildScreen.Converters"
             StartupUri="Windows/MainWindow.xaml">
  <Application.Resources>
    <converters:StatusToBrushConverter x:Key="StatusToBrushConverter" />
    <converters:MultiplyConverter x:Key="MultiplyConverter" />
    <converters:RelativeMarginConverter x:Key="RelativeMarginConverter" />
    <converters:BuildProgressWidthMultiConverter x:Key="PercentageMultiConverter" />
    <converters:EqualToOneToVisibleOrHiddenConverter x:Key="EqualToOneToVisibleOrHiddenConverter" />
    <converters:EqualToOneToHiddenOrVisibleConverter x:Key="EqualToOneToHiddenOrVisibleConverter" />
  </Application.Resources>
</Application>
